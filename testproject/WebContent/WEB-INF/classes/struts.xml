	<package name="default" extends="struts-default" namespace="/">
		<result-types>
			<result-type name="tiles"
				class="org.apache.struts2.views.tiles.TilesResult" />
		</result-types>

		<interceptors>

			<interceptor name="loggingInterceptor"
				class="com.kjnext.interceptor.LoggingInterceptor">
			</interceptor>
			<interceptor name="authenticationInterceptor"
				class="com.kjnext.interceptor.AuthenticationInterceptor">
			</interceptor>
			<interceptor name="transactionInterceptor"
				class="com.kjnext.interceptor.TransactionInterceptor" />
			<interceptor name="nonTransactionInterceptor"
				class="com.kjnext.interceptor.NonTransactionInterceptor" />

			<interceptor-stack name="transactionStack">
				<interceptor-ref name="loggingInterceptor" />
				<interceptor-ref name="authenticationInterceptor" />
				<interceptor-ref name="transactionInterceptor" />
				<interceptor-ref name="defaultStack" />
			</interceptor-stack>

			<interceptor-stack name="onlyTransactionNoAuthentication">
				<interceptor-ref name="loggingInterceptor" />
				<interceptor-ref name="transactionInterceptor" />
				<interceptor-ref name="defaultStack" />
			</interceptor-stack>

			<interceptor-stack
				name="NoTransactionNoAuthenticationOnlySession">
				<interceptor-ref name="loggingInterceptor" />
				<interceptor-ref name="nonTransactionInterceptor" />
				<interceptor-ref name="defaultStack" />
			</interceptor-stack>

			<interceptor-stack name="onlySessionAndAuthentication">
				<interceptor-ref name="loggingInterceptor" />
				<interceptor-ref name="authenticationInterceptor" />
				<interceptor-ref name="nonTransactionInterceptor" />
				<interceptor-ref name="defaultStack" />
			</interceptor-stack>

			<interceptor-stack name="loggingStack">
				<interceptor-ref name="loggingInterceptor" />
				<interceptor-ref name="defaultStack" />
			</interceptor-stack>

		</interceptors>

		<global-results>
			<result name="login">/common/login.jsp</result>
			<result name="unauthorized">
				/common/unauthorized.jsp
			</result>
			<result name="error">/common/error.jsp</result>
			<result name="applicationError">
